{
  "name": "@iiif/thumbnail-panel",
  "version": "1.0.0",
  "repository": "git@github.com:IIIF-Commons/thumbnail-panel.git",
  "license": "MIT",
  "main": "dist/bundle/cjs/index.js",
  "module": "dist/bundle/esm/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/bundle/cjs/index.js",
      "import": "./dist/bundle/esm/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "vite",
    "build": "node ./scripts/build.mjs",
    "prepublishOnly": "node ./scripts/build.mjs && node scripts/validate.mjs",
    "test": "vitest"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^8.1.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "3.0.1",
    "@vitest/coverage-c8": "^0.27.2",
    "chalk": "^5.2.0",
    "dts-bundle-generator": "^7.1.0",
    "eslint": "^8.32.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^6.1.0",
    "leva": "^0.9.34",
    "node-fetch": "^3.3.0",
    "prettier": "^2.8.3",
    "terser": "^5.16.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.27.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@iiif/parser": "^1.1.0",
    "@iiif/presentation-3": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
